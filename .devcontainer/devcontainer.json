{
       "name": "SQLite Dev",
       "dockerComposeFile": "docker-compose.yml",
       "service": "dev",
       "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
       "postCreateCommand": "pip install -r requirements.txt",
       "customizations": {
              "vscode": {
                     "extensions": [
                            "alexcvzz.vscode-sqlite",
                            "qwtel.sqlite-viewer"
                     ]
              }
       }
}